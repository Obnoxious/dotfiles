super + Q
	pkill swm
	
super + Return
	urxvt
	
super + space
	dmenu_run
super + x
	killw $(pfw)
super + Tab
	/home/perkele/bin/cycle.sh	
super + Escape
	kill -USR1 -x sxhkd

# to resize firefox to a divisible number
super + shift + r
	wtp `wattr xy $(pfw)` 880 780 `pfw`
	
super + Down
	wmv 0 10 $(pfw)
super + Up
	wmv 0 -10 $(pfw)
super + Right
	wmv 10 0 $(pfw)
super + Left
	wmv -10 0 $(pfw)
	
super + l
	wrs 10 0 $(pfw)
super + h
	wrs -10 0 $(pfw)
super + j
	wrs 0 -10 $(pfw)
super + k
	wrs 0 10 $(pfw)
